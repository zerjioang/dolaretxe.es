# nota: las lineas que empiezan con '#' son comentarios

# indicador del codigo de google analytics
# si esta vacio no se analiza nada
VUE_APP_GOOGLE_ANALYTICS='UA-10598589-1'

# true = muestra el boton de login en la barra de navegacion
# util para cuando haya una aplicacion de login
VUE_APP_SHOW_LOGIN_BUTTON=false

# true = muestra la pagina y el menu de trabaja con nosotros
VUE_APP_SHOW_WORK_WITH_US=false

# configuracion del estado de la matricula
VUE_APP_MATRICULA_ABIERTA=true
# fecha de fin de la matricula en formato YYYY-MM-DD (a√±o mes dia)
# ejemplo 2020-04-06
# la fecha de fin de matricula ya no es visible
VUE_APP_MATRICULA_FIN_FECHA=2020-04-06

# idioma por defecto
VUE_APP_I18N_LOCALE=es
# idioma en caso de no encontrar ninguno valido
VUE_APP_I18N_FALLBACK_LOCALE=es

# activar o desactivar el modo coronavirus de la web
VUE_APP_CORONAVIRUS_MODE_ON=true